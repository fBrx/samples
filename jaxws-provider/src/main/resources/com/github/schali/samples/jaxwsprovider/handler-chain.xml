<handler-chains xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee">
	<handler-chain>
		<handler>
			<handler-name>LoggingInHandler</handler-name>
			<handler-class>com.github.schali.jaxws.handler.LoggingInHandler</handler-class>
		</handler>
		<handler>
			<handler-name>LoggingOutHandler</handler-name>
			<handler-class>com.github.schali.jaxws.handler.LoggingOutHandler</handler-class>
		</handler>
		<handler>
			<handler-name>SpringSecurityHandler</handler-name>
			<handler-class>com.github.schali.jaxws.handler.SpringSecurityHandler</handler-class>
		</handler>
		
	</handler-chain>
</handler-chains>